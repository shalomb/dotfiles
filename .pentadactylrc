" vim: filetype=vim

set guioptions=nCrsN
set hlfind
set incfind
set newtab=all

autocmd PageLoad '^https?://www\.google\.(?:co.uk|com)' :normal! gi

noremap <C-f> <PageUp>

" Dump current Site w/o layout, format, etc. just plain text
map -e <A-l> :javascript dactyl.execute("!lynx -dump '" + dactyl.modules.buffer.URL + "' > '" + dactyl.modules.buffer.title + "'");
