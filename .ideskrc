table Config
  FontName: aquabase
  FontSize: 11
  FontColor: #EFEFEF
  HighContrast: True
  ToolTip.FontSize: 10
  ToolTip.FontName: gothic
  ToolTip.ForeColor: #FFFFFF
  ToolTip.BackColor: #000000
  ToolTip.CaptionOnHover: false
  ToolTip.CaptionPlacement: Left
  Locked: true 
  Transparency: 36
  Shadow: true
  ShadowColor: #000000
  ShadowX: 1
  ShadowY: 1 
  Bold: true
  ClickDelay: 300
  IconSnap: true
  SnapWidth: 36
  SnapHeight: 1
  SnapOrigin: BottomRight
  SnapShadow: false
  SnapShadowTrans: 128
  CaptionOnHover: true
  CaptionPlacement: top
  FillStyle: fillinvert
  Background.Delay: 0
  Background.Source: None
  Background.File: None
  Background.Mode: Center 
  Background.Color: #C2CCFF
